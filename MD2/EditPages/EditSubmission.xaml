<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MD2.EditPages.EditSubmission"
             Title="EditSubmission">
    <VerticalStackLayout Spacing="20" Padding="20">
        <Picker x:Name="StudentPicker" Title="Select Student" />
        <Picker x:Name="AssignmentPicker" Title="Select Assignment" />
        <DatePicker x:Name="SubmissionDatePicker" />
        <Entry x:Name="ScoreEntry" Placeholder="Enter Score" Keyboard="Numeric" />
        <!--<Button Text="Save Changes" Clicked="OnSaveSubmissionClicked" />-->
    </VerticalStackLayout>
</ContentPage>